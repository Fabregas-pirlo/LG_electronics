cell>\r\n      </mt-tab-container-item>\r\n      <mt-tab-container-item id=\"2\">\r\n        <mt-cell>\r\n          <div>\r\n            <div class=\"comments\">\r\n              <span>全部(0)</span>\r\n              <span>好评(0)</span>\r\n              <span>中评(0)</span>\r\n              <span>差评(0)</span>\r\n             </div>\r\n            <!-- <div class=\"temp\">暂无评价</div> -->\r\n          </div>\r\n          \r\n        </mt-cell>\r\n      </mt-tab-container-item>\r\n  \r\n    </mt-tab-container>\r\n    \r\n    <div class=\"bottom\">\r\n      <div></div>\r\n      <div class=\"addcart\">加入购物车</div>\r\n      <div class=\"buy\">立即购买</div>\r\n    </div>\r\n\r\n    <van-goods-action>\r\n      <van-goods-action-icon icon=\"chat-o\" text=\"客服\" />\r\n      \r\n      <van-goods-action-icon icon=\"cart-o\" text=\"购物车\" :info=\"$store.getters.getCartCount\" @click=\"tocart\"/>  \r\n      <van-goods-action-button type=\"warning\" text=\"加入购物车\" @click=\"addcart\" :data-lid=\"dataObj.lid\" :data-price=\"dataObj.price\" :data-lname=\"dataObj.lname\"/>\r\n      <van-goods-action-button type=\"danger\" text=\"立即购买\" @click=\"tocart\"/>\r\n    </van-goods-action>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default { \r\n  data() {\r\n    return {\r\n      dataObj: {},\r\n      // lid: 0,\r\n      selected: \"1\",\r\n    };\r\n  },\r\n  props: [\"lid\"],\r\n  methods: {\r\n    \r\n    addcart(e){\r\n      var lid=e.target.dataset.lid;\r\n      var n=e.target.dataset.lname;\r\n      var p=e.target.dataset.price;\r\n      var url = \"addcart\";\r\n      var obj = {lid:lid,lname:n,price:p};\r\n      this.axios.get(url,{params:obj})\r\n      .then(res=>{\r\n        if(res.data.code==-1){\r\n          this.$messagebox(\"消息\",\"请先登录\").then(res=>{\r\n            this.$router.push(\"/login\");\r\n          });\r\n        }else if(res.data.code==-2){\r\n          this.$toast(\"添加失败\");\r\n        }else{\r\n          this.$store.commit(\"increment\");\r\n          this.$toast(\"添加成功\");\r\n        }\r\n      })\r\n    },\r\n    tocart(){\r\n      this.$router.push(\"/cart\")\r\n    },\r\n    load() {\r\n      (async () => {\r\n        var result = await this.axios.get(\"http://localhost:8080/detail\", {\r\n          params: {\r\n            lid: this.lid\r\n          }\r\n        });\r\n        console.log(result.data);\r\n        this.dataObj = result.data.data[0];\r\n        // console.log(result.data.data)\r\n        \r\n      })();\r\n    }\r\n  },\r\n  created() {\r\n    this.load();\r\n    // this.lid = this.$route.lid;\r\n    console.log(this.lid);\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.boss{background: #e3e2de;position: relative;}\r\n.one{width:340px;display:block;}\r\n.two{width:340px;display:block;}\r\n.details1{\r\n  width:100%; \r\n}\r\n.details2{\r\n  width:100%;\r\n}\r\n.gohome{\r\n        display:block;\r\n        float:left;\r\n        background: url(../../../assets/toindex.png) no-repeat;\r\n        background-size:100%; \r\n        width:370px;\r\n        height: 40px;\r\n        margin: 5px auto;\r\n    }\r\n  .return{\r\n        display:block;\r\n        position: fixed;\r\n        background: url(../../../assets/return.png) no-repeat;\r\n        z-index: 10;\r\n        top:5px;\r\n        left: 7px;\r\n        width:40px;\r\n        height: 40px;\r\n        margin: 5px auto;\r\n    }\r\n\r\n.img01{\r\n  width:100% ;\r\n  height:360px;\r\n}\r\n.lname{\r\n  margin:5px 10px;\r\n}\r\n.price{\r\n  margin:5px 10px;\r\n  color:red;\r\n}\r\n.comments{display: block;\r\n          width:340px;}\r\n/* .temp{display: block;width:340px;} */\r\n.comments span{\r\n  padding:2px;\r\n  margin-left:5px;\r\n}\r\n.comments span:first-child{\r\n  width:12px;\r\n  height:3px;\r\n  border-radius:5px;\r\n  background: red;\r\n  color:#fff;\r\n}\r\n.comments span:nth-child(2){\r\n  width:12px;\r\n  height:3px;\r\n  border-radius:5px;\r\n  background: pink;\r\n  color:#000;\r\n}\r\n.comments span:nth-child(3){\r\n  width:12px;\r\n  height:3px;\r\n  border-radius:5px;\r\n  background: pink;\r\n  color:#000;\r\n}\r\n.comments span:nth-child(4){\r\n  width:12px;\r\n  height:3px;\r\n  border-radius:5px;\r\n  background: #e3e2de;\r\n  color:#000;\r\n}\r\n.bottom{\r\n  width:100%;\r\n  height:40px;\r\n  border:1px solid #000;\r\n  position:fixed;\r\n  bottom:0;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\boutique\\common"}]}